{
  "rules": {
    "string-quotes": "single",
    "number-leading-zero": "always",
    "rule-empty-line-before": [
      "always",
      {
        "ignore": [
          "after-comment",
          "first-nested",

        ]
      }
    ],
    "max-nesting-depth": 3,
  },
  "processors": [
    "stylelint-processor-styled-components"
  ],
  "extends": [
    "stylelint-config-airbnb",
    "stylelint-config-styled-components"
  ]
}
